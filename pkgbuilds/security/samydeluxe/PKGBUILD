pkgname=samydeluxe
pkgver=$(date +%y%m%d%H%M)
pkgrel=1
pkgdesc="A little python script for breaking windows SAM files"
arch=("i686")
url="http://github.com/jensp/chaox-ng-livecd/raw/master/code/samydeluxe"
license=('GPL')
depends=("python")
source=("http://github.com/jensp/chaox-ng-livecd/raw/master/code/samydeluxe")
build() {
  install -D -m755 "${srcdir}"/$pkgname "${pkgdir}"/usr/bin/$pkgname || return 1
}
